<!DOCTYPE html>
<html lang="ko">

<head>
    <!-- Required meta tags -->
    <%- include("./partialView/head", { title }); %>
    <%- include("./partialView/stylesheet") %>
</head>

<body id="mainBody" class="position-relative bg-gray-400">
    <!-- header -->
    <header id="mainHead" class="position-relative bg-gray-800">
        <%- include("./navigation/createNavigationContainer", { security }); %>
    </header><!-- /END header -->
    <!-- main -->
    <main id="mainContent">
        <!-- Notification, Ad, post -->
        <div class="container">
            <div class="row">
                <div class="col mx-1 px-3">
                    <!-- Alert -->
                    <% if (context.alert) { %>
                    <a href="<%= context.alert.alert_link ? context.alert.alert_link : '#' %>" class="card shadow rounded-lg border-0 px-3 mb-2 footer text-decoration-none">
                        <div class="card-body px-1 pb-2 mb-1">
                            <h5 class="card-title small lh-1 fw-bold mb-2 text-black">
                                알림
                            </h5>
                            <div class="d-flex flex-row">
                                <p class="card-text lh-medium ls-xs mb-0 text-secondary fw-normal"><%-
                                            context.alert.alert_message %></p>
                            </div>
                        </div>
                    </a><!-- /END Alert -->
                    <% } %>
                    <!-- Fixed post -->
                    <% context.notices.forEach((element, index)=> { %>
                    <div class="card p-4 border-0 pt-3 rounded-lg shadow 
                        <%= (index !== 0) ? " mt-4" : "" %>
                                    <%= (index + 1===context.notices.length ? "mb-2" : "" ) %>" onclick="location.href=`/notices/<%= element.notice_id %>`">
                        <div class="card-body p-0 pt-1 position-relative">
                            <small class="card-subtitle mb-2 text-secondary fw-600">
                                <%= element.notice_subtitle %>
                            </small>
                            <h5 class="card-title fw-bold lh-medium ls-xs pt-1 mb-0">
                                <%= element.notice_title %>
                            </h5>
                            <small class="fs-7 fw-normal text-secondary">
                                <%= element.notice_created_at %>
                            </small>
                            <% if (element.template_brand_thumbnail) { %>
                            <span role="image" aria-label="<%= element.template_brand_thumbnail %>" class="display-lg position-absolute bottom-0 end-0" style="font-family: 'Apple Color Emoji', 'Segoe UI Emoji', NotoColorEmoji, 'Noto Color Emoji', 'Segoe UI Symbol', 'Android Emoji', EmojiSymbols; line-height: 1em;">
                                <%= element.template_brand_thumbnail %>
                            </span>
                            <% } %>
                        </div>
                    </div>
                    <% }); %>
                    <!-- /END Fixed post -->
                    <!-- Sponsored ad -->
                    <div class="px-4 pt-2 pb-3 mb-2 position-relative">
                        <div class="pt-4">
                            <h6 class="fw-bold ls-sm mb-0">
                                2024년 14회 내구페스티발
                            </h6>
                            <small class="fw-600 ls-xs">모토스타와 함께합니다.</small>
                        </div>
                        <img src="/images/static/ggr.png" class="position-absolute end-0 sponsored-ad-img" />
                    </div><!-- /END Sponsored ad -->
                </div>
            </div>
        </div><!-- /END Notification, Ad, post -->
        <!-- Main Poster -->
        <div class="container">
            <div class="row">
                <div class="col px-0">
                    <img src="/images/static/2024_main_poster.png" alt="" class="w-100">
                </div>
            </div>
        </div>
        <!-- /END Main Poster -->
        <!-- calendar -->
        <% function getStyleClass(prog_id) { let styleClass=[]; switch (prog_id) { case 1:
            styleClass.push("bg-gray-200"); styleClass.push("text-dark"); break; case 2: styleClass.push("bg-info");
            styleClass.push("text-white"); break; case 3: styleClass.push("bg-secondary");
            styleClass.push("text-white"); break; } return styleClass.join(" ");
            };
        %>
        <div class=" container pt-4">
            <div class="row">
                <div class="col mx-1 px-3 pt-3">
                    <h2 class="ls-sm fw-bold lh-sm">
                        2024년 모토스타 14회<br />KSEF 내구페스티발 일정
                    </h2>
                    <p class="pt-2 pb-1 mb-3 text-black">
                        모토스타 함께<br />뜨거운 여름을 준비하고 있습니다!
                    </p>
                    <div class="d-flex flex-column-reverse position-relative">
                        <input type="checkbox" class="btn-check btn-link" id="btncheck1" autocomplete="off" />
                        <label class="btn mt-3 lh-1 h6 py-3 fw-normal text-decoration-underline text-secondary" for="btncheck1"></label>
                        <% const show=context.calendar.slice(1); const hide=context.calendar.slice(0, 1); %>
                        <div class="list-group flex-column-reverse rounded-0" style="display: none">
                            <% hide.forEach((element)=> { %>
                            <div class="list-group-item d-flex flex-row px-3 py-2 align-items-center rounded-5 border-0 bg-white mb-1 shadow-sm">
                                <span class="badge <%= getStyleClass(element.prog_id) %>">
                                    <small class="lh-1 py-1">
                                        <%= element.prog_name %>
                                    </small>
                                </span>
                                <div class="py-1 d-flex flex-column px-1 mx-2">
                                    <h6 class="mb-2 lh-1 fw-600">
                                        <%= element.cal_name %>
                                    </h6>
                                    <small class="lh-1 float-start text-secondary">
                                        <%= element.cal_date %>
                                    </small>
                                </div>
                            </div>
                            <% }); %>
                        </div>
                        <div class="list-group flex-column-reverse rounded-0">
                            <% show.forEach((element)=> { %>
                            <div class="list-group-item d-flex flex-row px-3 py-2 align-items-center rounded-5 border-0 bg-white mb-1 shadow-sm">
                                <span class="badge <%= getStyleClass(element.prog_id) %>"><small class="lh-1 py-1">
                                        <%= element.prog_name %>
                                    </small></span>
                                <div class="py-1 d-flex flex-column px-1 mx-2">
                                    <h6 class="mb-2 lh-1 fw-600">
                                        <%= element.cal_name %>
                                    </h6>
                                    <small class="lh-1 float-start text-secondary">
                                        <%= element.cal_date %>
                                    </small>
                                </div>
                            </div>
                            <% }); %>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- /END calendar -->
