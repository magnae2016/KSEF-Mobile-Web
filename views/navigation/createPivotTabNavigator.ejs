<nav id="<%= locals.id && locals.id %>" class="navbar navbar-expand-lg navbar-light flex-row py-0 position-relative overflow-hidden w-100 bg-white zindex-fixed">
    <div class="container-fluid flex-row py-2 px-3">
        <ul class="nav nav-pills border-start-1 border-transparent">
            <% _items.forEach(element => { %>
            <li class="nav-item pe-1 border border-transparent">
                <a href="<%= element.link %>" class="nav-link rounded-pill <%= selectedKey === `key${element.headerText}` ? "active" : "" %>" href="#"><%= element.headerText %></a>
            </li>
            <% }) %>
        </ul>
        <select id="NoticeYearFiltering" class="form-select" aria-label="연도 선택">
            <% years.forEach(element=> { %>
                <option value="<%= element %>" <%=element==selectedYear ? "selected" : "" %>><%= element %>
                </option>
                <% }) %>
        </select>
    </div>
</nav>